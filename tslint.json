{
  "defaultSeverity": "warning",
  "linterOptions": {
    "exclude": [
        "**/*.js"
    ]
  },
  "extends": [
      "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "no-namespace": [true, "allow-declarations"]
  },
  "rulesDirectory": []
}
