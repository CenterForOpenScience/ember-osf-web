{{yield (hash
    selected=this.selected
    onChange=(action this.onChange)
    hasChildren=this.hasChildren
    toggleChildren=(action this.toggleChildren)
    children=this.children
    shouldShowChildren=this.shouldShowChildren
    subject=this.subject
    loading=(and this.loading (not this.loadingMoreChildren))
    subjectManager=@manager
    childrenCount=this.childrenCount
    showMore=(action this.showMore)
    hasMore=this.hasMore
    loadingMoreChildren=this.loadingMoreChildren
    placeholderCount=this.placeholderCount
)}}
