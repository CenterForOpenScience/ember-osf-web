<div local-class='BrandContainer' {{with-branding this.brand}}>
    <div local-class='PrimaryColor'></div>
    <Navbar
        data-analytics-scope='Navbar'
        @dark={{true}}
        as |nav|
    >
        <nav.container>
            {{! Left Side (Band, Service Dropdown) }}
            <nav.section data-test-left>
                <OsfLink
                    data-test-brand
                    data-analytics-name='Brand'
                    @route='home'
                    local-class='Logo'
                    class='branded-logo'
                    aria-label={{t 'navbar.go_home'}}
                />
                <OsfLink @route='registries.branded'>Test Link</OsfLink>
            </nav.section>

        </nav.container>
    </Navbar>
</div>