{{page-title (t 'registries.discover.page_title') prepend=false}}
<RegistriesWrapper
    @provider={{this.model}}
    {{with-branding (get-model this.model.brand)}}
    data-analytics-scope='Registries Discover page'
>
    <SearchPage
        @provider={{this.model}}
        @defaultQueryOptions={{this.defaultQueryOptions}}
        @resourceType={{'Registration,RegistrationComponent'}}
        @queryParams={{this.queryParams}}
        @query={{this.q}}
        @sort={{this.sort}}
        @onQueryParamChange={{action this.onQueryParamChange}}
        @showResourceTypeFilter={{false}}
        @activeFilters={{this.activeFilters}}
    />
</RegistriesWrapper>
