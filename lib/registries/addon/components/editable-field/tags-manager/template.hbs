{{yield (hash
    save=(action this.save)
    cancel=(action this.cancel)
    addTag=(action this.addTag)
    removeTag=(action this.removeTag)
    clickTag=(action this.clickTag)
    tags=this.currentTags
    shouldShowEmptyFieldText=this.shouldShowEmptyFieldText
    shouldShowTitle=this.shouldShowTitle
    fieldChanged=this.fieldChanged
    fieldIsEmpty=this.fieldIsEmpty
    emptyFieldText=(t 'osf-components.tags-widget.no_tags')
)}}
