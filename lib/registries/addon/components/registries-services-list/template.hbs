<div local-class='service-list-container'>
    <div local-class='header-container'>
        <h2 data-test-registries-list-header>
            {{t 'registries.index.services.top.heading'}}
        </h2>
        <p data-test-registries-list-paragraph>
            {{t 'registries.index.services.top.paragraph'}}
        </p>
    </div>
    <div local-class='service-container'>
        <div
            data-test-registries-list-row-one
            local-class='service-row-container {{if this.isMobile 'mobile'}}'
        >
            <div local-class='service-25'>
                <a
                    href='https://osf.io/registries/darpaasist' 
                    data-analytics-name='asist - Registry'
                    data-test-asist-registry
                    aria-label={{t 'registries.index.services.asist'}}
                >
                    <img
                        alt={{concat (t 'registries.index.services.asist') (t 'registries.index.services.logo')}}
                        local-class='logo'
                        data-test-asist-registry-logo
                        src='/img/provider_logos/ASIST_logo.png'
                    />
                </a>
            </div>
            <div local-class='service-25'>
                <a
                    href='https://osf.io/registries/characterlabregistry' 
                    data-analytics-name='characterlab - Registry'
                    data-test-characterlab-registry
                    aria-label={{t 'registries.index.services.characterlab'}}
                >
                    <img
                        alt={{concat (t 'registries.index.services.characterlab') (t 'registries.index.services.logo')}}
                        local-class='logo'
                        data-test-characterlab-registry-logo
                        src='/img/provider_logos/CharacterLab_logo.png'
                    />
                </a>
            </div>
            <div local-class='service-25'>
                <a
                    href='https://osf.io/registries/dam'
                    data-analytics-name='dam - Registry'
                    data-test-dam-registry
