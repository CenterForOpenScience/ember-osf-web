    <div local-class='contact-container'>
        <a
            data-test-registries-list-contact-link
            data-analytics-name='Index - Contact'
            href='mailto:contact@osf.io'
            local-class='btn'
        >
            {{t 'registries.index.services.bottom.contact'}}
        </a>
    </div>
</div>
