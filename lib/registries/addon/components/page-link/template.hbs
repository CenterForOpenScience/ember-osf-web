<@link
    data-analytics-name={{this.IndexWithSlug}}
    local-class='{{this.pageClass}}'
    @route='registries.drafts.draft'
    @models={{array this.draftId this.pageIndexWithSlug}}
    @icon={{ this.pageIcon}}
    @label={{this.pageManager.pageHeadingText}}
    @isCurrentPage={{this.pageIsActive}}
    as |link|
>
    <link.icon />
    <link.label
        local-class='.Label'
    />
</@link>