<@link
    data-analytics-name={{this.pageLabel}}
    data-test-link={{this.page}}
    local-class='{{this.pageClass}}'
    @route='registries.drafts.draft.page'
    @models={{array this.draftId this.page}}
    @icon={{this.pageIcon}}
    @label={{this.pageLabel}}
    @isDrawer={{this.isDrawer}}
    as |link|
>
    {{compute (fn this.setIsActive) link.isActive}}

    <link.icon />
    <link.label local-class='Label' />
</@link>