{{page-title (t 'registries.discover.page_title') prepend=false}}
<RegistriesNavbar
    class='RegistriesStyle'
    @campaign='{{if @provider @provider.id 'osf'}}-registries'
    @provider={{this.model}}
    {{with-branding (get-model this.model.brand)}}
/>

<SearchPage
    {{with-branding (get-model this.model.brand)}}
    @route='discover'
    @provider={{this.model}}
    @defaultQueryOptions={{this.defaultQueryOptions}}
    @queryParams={{this.queryParams}}
    @query={{this.q}}
    @sort={{this.sort}}
    @onSearch={{action this.onSearch}}
    @showResourceTypeFilter={{false}}
/>