{{assert 'Registries::ReviewFormRenderer requires a draftManager' @draftManager}}
{{#each @draftManager.pageManagers as |pageManager|}}
    {{#each pageManager.schemaBlockGroups as |group|}}
        <Registries::SchemaBlockGroupRenderer
            @schemaBlockGroup={{group}}
            @renderStrategy={{
                component
                'registries/schema-block-renderer/read-only/mapper'
                registrationResponses=@draftManager.draftRegistration.registrationResponses
                changeset=pageManager.changeset
                draftManager=@draftManager
            }}
            @showEditButton={{@showEditButton}}
        />
    {{/each}}
{{/each}}