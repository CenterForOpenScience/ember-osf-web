{{assert 'Registries::ReviewFormRenderer requires a draftManager' @draftManager}}
{{assert 'Registries::ReviewFormRenderer requires a node' @node}}
{{#each @draftManager.pageManagers as |pageManager pageIndex|}}
    {{#each pageManager.schemaBlockGroups as |group groupIndex|}}
        <Registries::SchemaBlockGroupRenderer
            @schemaBlockGroup={{group}}
            @renderStrategy={{
                component
                'registries/schema-block-renderer/read-only/mapper'
                registrationResponses=@draftManager.registrationResponses
                node=@node
                changeset=pageManager.changeset
            }}
        />
        {{#if (and (eq pageIndex 0) (eq groupIndex 0))}}
            <Registries::RegistrationFormNavigationDropdown @schemaBlocks={{@draftManager.schemaBlocks}} />
        {{/if}}
    {{/each}}
{{/each}}