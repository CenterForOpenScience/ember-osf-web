{{#if this.fetchSchemaBlocks.isRunning}}
    <LoadingIndicator @dark={{true}}/>
{{else}}
    {{#if (gt this.registration.revisions.length 1)}}
        <Registries::VersionMetadata @registration={{this.registration}} @revision={{this.revision}} />
    {{/if}}
    <Registries::RegistrationFormNavigationDropdown
        @showMetadata={{false}}
        @schemaBlocks={{this.schemaBlocks}}
    />
    {{#each this.schemaBlockGroups as |group|}}
        <Registries::SchemaBlockGroupRenderer
            @schemaBlockGroup={{group}}
            @renderStrategy={{
                component
                'registries/schema-block-renderer/read-only/mapper'
                registrationResponses=this.responses
            }}
        />
    {{/each}}
{{/if}}