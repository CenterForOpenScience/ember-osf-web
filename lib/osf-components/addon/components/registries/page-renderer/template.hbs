{{#each @pageManager.schemaBlockGroups as |group|}}
    <Registries::SchemaBlockGroupRenderer
        @schemaBlockGroup={{group}}
        @renderStrategy={{
            component 'registries/schema-block-renderer/editable/mapper'
                changeset=this.pageManager.changeset
                node=@node
                onInput=@onInput
                draftRegistration=@draftRegistration
        }}
    />
{{/each}}
