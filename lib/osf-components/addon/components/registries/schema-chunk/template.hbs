{{yield (hash
    page-heading=(
        component
        'registries/schema-chunk/page-heading'
    )
    section-heading=(
        component
        'registries/schema-chunk/section-heading'
    )
    subsection-heading=(
        component
        'registries/schema-chunk/subsection-heading'
    )
    paragraph=(
        component
        'registries/schema-chunk/paragraph'
    )
    label=(
        component
        'registries/schema-chunk/label'
        answerId=@answerId
    )
    text=(
        component
        'registries/schema-chunk/text'
        answerId=@answerId
        changeset=@changeset
        disabled=this.disabled
        shouldShowMessages=this.shouldShowMessages
    )
    textarea=(
        component
        'registries/schema-chunk/textarea'
        answerId=@answerId
        changeset=@changeset
        disabled=this.disabled
        shouldShowMessages=this.shouldShowMessages
    )
)}}