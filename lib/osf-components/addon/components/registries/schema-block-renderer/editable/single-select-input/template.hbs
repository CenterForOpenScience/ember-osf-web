<FormControls
    @changeset={{@changeset}}
    @disabled={{@disabled}}
    @shouldShowMessages={{@shouldShowMessages}}
    ...attributes
    as |form|
>
    <form.select
        data-test-single-select-input
        @id={{@uniqueID}}
        @valuePath={{@schemaBlock.registrationResponseKey}}
        @options={{this.optionBlockValues}}
        @onchange={{@onInput}}
        as |option|
    >
        {{option}}
    </form.select>
</FormControls>
