{{yield (hash
    page-heading=(
        component
        'registries/schema-block-renderer/page-heading'
    )
    section-heading=(
        component
        'registries/schema-block-renderer/section-heading'
    )
    subsection-heading=(
        component
        'registries/schema-block-renderer/subsection-heading'
    )
    paragraph=(
        component
        'registries/schema-block-renderer/paragraph'
    )
    question-label=(
        component
        'registries/schema-block-renderer/label'
        readonly=true
    )
    short-text-input=(
        component
        'registries/schema-block-renderer/read-only/response'
        registrationResponses=@registrationResponses
    )
    long-text-input=(
        component
        'registries/schema-block-renderer/read-only/response'
        registrationResponses=@registrationResponses
    )
    single-select-input=(
        component
        'registries/schema-block-renderer/read-only/response'
        registrationResponses=@registrationResponses
    )
    multi-select-input=(
        component
        'registries/schema-block-renderer/read-only/response'
        registrationResponses=@registrationResponses
    )
    contributors-input=(
        component
        'registries/schema-block-renderer/read-only-contributor-list'
        node=@node
    )
    file-input=(
        component
        'registries/schema-block-renderer/read-only/files'
        registrationResponses=@registrationResponses
    )
)}}