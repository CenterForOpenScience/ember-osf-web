{{yield (hash
    page-heading=(
        component
        'registries/schema-block-renderer/page-heading'
        schemaBlock=@schemaBlock
    )
    section-heading=(
        component
        'registries/schema-block-renderer/section-heading'
        schemaBlock=@schemaBlock
    )
    subsection-heading=(
        component
        'registries/schema-block-renderer/subsection-heading'
        schemaBlock=@schemaBlock
    )
    paragraph=(
        component
        'registries/schema-block-renderer/paragraph'
        schemaBlock=@schemaBlock
    )
    question-label=(
        component
        'registries/schema-block-renderer/label'
        schemaBlock=@schemaBlock
    )
    short-text-input=(
        component
        'registries/schema-block-renderer/text'
        schemaBlock=@schemaBlock
        changeset=@changeset
        disabled=@disabled
        shouldShowMessages=@shouldShowMessages
    )
    long-text-input=(
        component
        'registries/schema-block-renderer/textarea'
        schemaBlock=@schemaBlock
        changeset=@changeset
        disabled=@disabled
        shouldShowMessages=@shouldShowMessages
    )
    single-select-input=(
        component
        'registries/schema-block-renderer/single-select-input'
        schemaBlock=@schemaBlock
        changeset=@changeset
        options=@options
        disabled=@disabled
        shouldShowMessages=@shouldShowMessages
    )
    multi-select-input=(
        component
        'registries/schema-block-renderer/multi-select-input'
        schemaBlock=@schemaBlock
        changeset=@changeset
        options=@options
        disabled=@disabled
        shouldShowMessages=@shouldShowMessages
    )
    contributor-list=(
        component
        'registries/schema-block-renderer/read-only-contributor-list'
        schemaBlock=@schemaBlock
        node=@node
    )
)}}