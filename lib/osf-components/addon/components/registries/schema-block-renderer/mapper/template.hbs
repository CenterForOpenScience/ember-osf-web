{{yield (hash
    page-heading=(
        component
        'registries/schema-group-renderer/page-heading'
        schemaBlock=@schemaBlock
    )
    section-heading=(
        component
        'registries/schema-group-renderer/section-heading'
        schemaBlock=@schemaBlock
    )
    subsection-heading=(
        component
        'registries/schema-group-renderer/subsection-heading'
        schemaBlock=@schemaBlock
    )
    paragraph=(
        component
        'registries/schema-group-renderer/paragraph'
        schemaBlock=@schemaBlock
    )
    label=(
        component
        'registries/schema-group-renderer/label'
        schemaBlock=@schemaBlock
    )
    short-text-input=(
        component
        'registries/schema-group-renderer/text'
        schemaBlock=@schemaBlock
        changeset=@changeset
        disabled=@disabled
        shouldShowMessages=@shouldShowMessages
    )
    long-text-input=(
        component
        'registries/schema-group-renderer/textarea'
        schemaBlock=@schemaBlock
        changeset=@changeset
        disabled=@disabled
        shouldShowMessages=@shouldShowMessages
    )
    single-select-input=(
        component
        'registries/schema-group-renderer/single-select-input'
        schemaBlock=@schemaBlock
        changeset=@changeset
        options=@options
        disabled=@disabled
        shouldShowMessages=@shouldShowMessages
    )
    multi-select-input=(
        component
        'registries/schema-group-renderer/multi-select-input'
        schemaBlock=@schemaBlock
        changeset=@changeset
        options=@options
        disabled=@disabled
        shouldShowMessages=@shouldShowMessages
    )
)}}