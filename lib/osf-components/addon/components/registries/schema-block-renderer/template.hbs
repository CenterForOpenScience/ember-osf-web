<SchemaBlockRenderer::Mapper as |renderers|>
    {{#let (get renderers @block.blockType) as |Renderer|}}
        <Renderer
            @changeset={{@changeset}}
            @schemaBlock={{@schemaBlock}}
            @disabled={{this.disabled}}
            @shouldShowMessages={{this.shouldShowMessages}}
        />
    {{/let}}
</SchemaBlockRenderer::Mapper>