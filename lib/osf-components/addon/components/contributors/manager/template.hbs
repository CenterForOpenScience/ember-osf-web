{{yield (hash
    contributors=this.contributors
    fetchContributors=(perform this.fetchContributors)
    toggleContributorIsBibliographic=(perform this.toggleContributorIsBibliographic)
    updateContributorPermission=(perform this.updateContributorPermission)
    reorderContributor=(perform this.reorderContributor)
    isLoading=this.fetchContributors.isRunning
    isDragging=this.isDragging
    shouldShowLoadMore=this.shouldShowLoadMore
    hasMore=this.hasMore
)}}
