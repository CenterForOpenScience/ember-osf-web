<ul class='list-group'>
    {{#paginated-list/has-many
        modelTaskInstance=@modelTaskInstance
        relationshipName=@relationshipName
        query=this.queryParams
        bindReload=this.bindReload
        as |pr|
    }}
        <pr.item as |node|>
            <NodeCard
                @node={{node}}
                @showTags={{@showTags}}
            />
        </pr.item>

        {{yield (hash empty=pr.empty)}}
    {{/paginated-list/has-many}}
</ul>
