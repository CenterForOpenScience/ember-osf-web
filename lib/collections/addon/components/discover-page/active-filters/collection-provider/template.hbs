{{#each @context.activeFilter as |item|}}
    <DiscoverPage::ActiveFilters::Filter
        @text={{filter-replace item.key @filterReplace}}
        @ariaLabel={{t 'collections.discover_page.active_filters.remove_provider'}}
        @hide={{this.theme.isProvider}}
        @removeFilterItem={{action this.removeFilterItem item}}
    />
{{/each}}