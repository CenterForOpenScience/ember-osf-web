<div class="container">
    <div class="navbar-header">
        <a class="navbar-brand">
            <span class="navbar-image" local-class="navbar-image"></span>
            <span class="navbar-title">{{brandTitle}}</span>
        </a>
    </div>
    {{#bs-collapse
        elementId='secondary-navigation'
        classNames='navbar-collapse navbar-right'
        collapsed=(not showNavLinks)
    }}
        <ul class="nav navbar-nav">
            <li>
                <a
                    href={{myProjectsUrl}}
                    onclick={{action 'click' 'link' 'Navbar - navbar.my_projects' target=analytics}}
                >
                    {{t 'branded_navbar.my_osf_projects'}}
                </a>
            </li>
            <li>
                {{#link-to
                    'provider.submit'
                    click=(action 'click' 'link' (concat 'Navbar - Add' objectType) target=analytics)
                }}
                    {{t 'collections.navbar.add_item'}}
                {{/link-to}}
            </li>
            <li>
                {{#link-to
                    'provider.discover'
                    click=(action 'click' 'link' 'Navbar - Search' target=analytics)
                }}
                    {{t 'navbar.search'}}
                {{/link-to}}
            </li>
            <li>
                <a href="https://cos.io/donate" onclick={{action 'click' 'link' 'Navbar - Donate' target=analytics}}>
                    {{t 'navbar.donate'}}
                </a>
            </li>
            {{osf-navbar/auth-dropdown
                signupUrl=signupUrl
                loginAction=loginAction
                redirectUrl=redirectUrl
                onLinkClicked=(action (mut showNavLinks) false)
                headline=(t 'branded_navbar.on_the_osf')
                externalLink=true
            }}
        </ul>
    {{/bs-collapse}}
</div>
