{
  "compilerOptions": {
    "target": "es2017",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmitOnError": true,
    "noEmit": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "baseUrl": ".",
    "module": "es6",
    "paths": {
      "ember-osf-web/tests/*": [
        "tests/*"
      ],
      "ember-osf-web/*": [
        "app/*"
      ],
      "ember-osf-web/mirage/*": [
        "mirage/*"
      ],
      "app-components": [
        "lib/app-components/addon"
      ],
      "app-components/*": [
        "lib/app-components/addon/*"
      ],
      "osf-components": [
        "lib/osf-components/addon"
      ],
      "osf-components/*": [
        "lib/osf-components/addon/*"
      ],
      "config/*": [
        "config/*"
      ],
      "ember-bootstrap/components/*": [
        "node_modules/ember-bootstrap/addon/components/bs3/*",
        "node_modules/ember-bootstrap/addon/components/bs4/*"
      ],
      "*": [
        "types/*"
      ]
    },
    "experimentalDecorators": true,
    "skipLibCheck": true
  },
  "include": [
    "app",
    "config",
    "tests",
    "mirage",
    "types",
    "lib/app-components",
    "lib/osf-components"
  ]
}
