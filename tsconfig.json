{
  "compilerOptions": {
    "target": "es2020",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmitOnError": false,
    "noEmit": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "skipLibCheck": true,
    "baseUrl": ".",
    "module": "es6",
    "experimentalDecorators": true,
    "paths": {
      "ember-osf-web/tests/*": [
        "tests/*"
      ],
      "ember-osf-web/mirage/*": [
        "mirage/*"
      ],
      "ember-osf-web/*": [
        "app/*",
        "lib/analytics-page/app/*",
        "lib/collections/app/*",
        "lib/osf-components/app/*",
        "lib/registries/app/*",
        "lib/app-components/app/*"
      ],
      "analytics-page": [
        "lib/analytics-page/addon"
      ],
      "analytics-page/*": [
        "lib/analytics-page/addon/*"
      ],
      "app-components": [
        "lib/app-components/addon"
      ],
      "app-components/*": [
        "lib/app-components/addon/*"
      ],
      "collections": [
        "lib/collections/addon"
      ],
      "collections/*": [
        "lib/collections/addon/*"
      ],
      "collections/config/*": [
        "lib/collections/config/*"
      ],
      "osf-components": [
        "lib/osf-components/addon"
      ],
      "osf-components/*": [
        "lib/osf-components/addon/*"
      ],
      "registries": [
        "lib/registries/addon"
      ],
      "registries/*": [
        "lib/registries/addon/*"
      ],
      "registries/config/*": [
        "lib/registries/config/*"
      ],
      "*": [
        "types/*"
      ]
    }
  },
  "include": [
    "app/**/*",
    "tests/**/*",
    "config/*",
    "types/**/*",
    "lib/analytics-page/**/*",
    "lib/app-components/**/*",
    "lib/collections/**/*",
    "lib/osf-components/**/*",
    "lib/registries/**/*",
    "mirage/**/*"
  ],
  "exclude": [
    "handbook-docs/**/*"
  ]
}
