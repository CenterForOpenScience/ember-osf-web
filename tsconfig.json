{
  "compilerOptions": {
    "target": "es2017",
    "allowJs": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmitOnError": true,
    "noEmit": true,
    "inlineSourceMap": true,
    "inlineSources": true,
    "baseUrl": ".",
    "module": "es6",
    "paths": {
      "ember-responsive/test-support": [
        "node_modules/ember-responsive/addon-test-support"
      ],
      "collections/*": [
        "lib/collections/addon/*"
      ],
      "registries/*": [
        "lib/registries/addon/*"
      ],
      "registries/config/*": [
        "lib/registries/config/*"
      ],
      "ember-osf-web/tests/*": [
        "tests/*"
      ],
      "ember-osf-web/*": [
        "app/*"
      ],
      "ember-osf-web/mirage/*": [
        "mirage/*"
      ],
      "osf-components": [
        "lib/osf-components/addon"
      ],
      "osf-components/*": [
        "lib/osf-components/addon/*"
      ],
      "handbook": [
        "lib/handbook/addon"
      ],
      "handbook/*": [
        "lib/handbook/addon/*"
      ],
      "analytics-page": [
        "lib/analytics-page/addon"
      ],
      "analytics-page/*": [
        "lib/analytics-page/addon/*"
      ],
      "config/*": [
        "config/*"
      ],
      "npm:*": [
        "node_modules/*",
        "node_modules/@types/*"
      ],
      "ember-bootstrap/components/*": [
        "node_modules/ember-bootstrap/addon/components/bs3/*",
        "node_modules/ember-bootstrap/addon/components/bs4/*"
      ],
      "*": [
        "types/*"
      ]
    },
    "experimentalDecorators": true,
    "skipLibCheck": true
  },
  "include": [
    "app",
    "config",
    "tests",
    "mirage",
    "types",
    "lib/analytics-page",
    "lib/collections",
    "lib/handbook",
    "lib/osf-components",
    "lib/registries"
  ]
}
