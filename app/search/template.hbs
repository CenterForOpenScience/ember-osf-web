{{page-title (t 'search.page-title')}}

<SearchPage
    @route='search'
    @cardSearchText={{this.cardSearchText}}
    @queryParams={{this.queryParams}}
    @onSearch={{action this.onSearch}}
    @showResourceTypeFilter={{true}}
    @sort={{this.sort}}
    @resourceType={{this.resourceType}}
    @page={{this.page}}
/>
