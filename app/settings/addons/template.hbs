{{#let
    (component 'settings/addons/-components/addon-section')
    (component 'paginated-list/all')
    as |AddonSection List|
}}
    <Panel as |panel|>
        <panel.heading @title={{t 'settings.addons.title'}} />
        <panel.body>
            <List @modelName='addon' as |list|>
                <list.item as |addon|>
                    <AddonSection @addon={{addon}} />
                </list.item>
            </List>
        </panel.body>
    </Panel>
{{/let}}
