<div class="row file-row-item" onclick={{action 'open' preventDefault='true'}} role="option">
    <div class="col-lg-12 file-row-col col-md-9 col-sm-9 col-xs-12 file-browser-header">
        {{file-icon item=item}}
        <a {{action 'open' preventDefault='true'}} role="link" href={{link}}>
            {{item.itemName}}
        </a>
    </div>
</div>
